//REMOVE ".example from file name"

{
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=(NAME_OF_DATABASE);user=(USERNAME);password=(PASSWORD);"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "Secret": "(SECRET_TOKEN_SIGNATURE)"
  }
}
